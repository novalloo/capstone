<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".detail.History">

    <TextView
        android:id="@+id/tv_history"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/history"
        android:textColor="@color/black"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.118"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.06" />

    <androidx.cardview.widget.CardView
        android:id="@+id/hst_squat"
        android:layout_width="370dp"
        android:layout_height="80dp"
        app:cardBackgroundColor="@color/skyblue"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.19">

        <TextView
            android:id="@+id/tv_hst_squat"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/squat_jump"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_marginTop="8dp"/>

        <TextView
            android:id="@+id/tv_hst_squat2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            tools:text="@string/_20_times"
            android:textColor="@color/black"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="36dp"/>

        <TextView
            android:id="@+id/tv_hst_squat3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            tools:text="@string/_200_reps"
            android:textColor="@color/black"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="150dp"/>

        <TextView
            android:id="@+id/tv_hst_squat4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="270dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="16dp"
            android:fontFamily="@font/poppins_regular"
            android:textColor="@color/black"
            tools:text="@string/_200_reps" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/hst_dumbbell"
        android:layout_width="370dp"
        android:layout_height="80dp"
        app:cardBackgroundColor="@color/skyblue"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hst_squat"
        app:layout_constraintVertical_bias="0.19">

        <TextView
            android:id="@+id/tv_hst_dumbbell"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/dumbbell"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:layout_marginTop="8dp"/>

        <TextView
            android:id="@+id/tv_hst_dumbbell2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            tools:text="@string/_20_times"
            android:textColor="@color/black"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="36dp"/>

        <TextView
            android:id="@+id/tv_hst_dumbbell3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/poppins_regular"
            tools:text="@string/_200_reps"
            android:textColor="@color/black"
            android:layout_marginTop="50dp"
            android:layout_marginLeft="150dp"/>

        <TextView
            android:id="@+id/tv_hst_dumbbell4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="270dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="16dp"
            android:fontFamily="@font/poppins_regular"
            android:textColor="@color/black"
            tools:text="@string/_200_reps" />

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/hst_pushup"
        android:layout_width="370dp"
        android:layout_height="80dp"
        app:cardBackgroundColor="@color/skyblue"
        app:cardCornerRadius="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/hst_dumbbell"
        app:layout_constraintVertical_bias="0.236">

        <TextView
            android:id="@+id/tv_hst_pushup"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/poppins_bold"
            android:text="@string/push_up"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/tv_hst_pushup2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="50dp"
            android:fontFamily="@font/poppins_regular"
            android:textColor="@color/black"
            tools:text="@string/_20_times" />

        <TextView
            android:id="@+id/tv_hst_pushup3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="50dp"
            android:fontFamily="@font/poppins_regular"
            android:textColor="@color/black"
            tools:text="@string/_200_reps" />

        <TextView
            android:id="@+id/tv_hst_pushup4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="270dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="16dp"
            android:fontFamily="@font/poppins_regular"
            android:textColor="@color/black"
            tools:text="@string/_200_reps" />

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>